//Declarative pipeline

pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				echo "Build"
			}
		}
		stage('Test') {
			steps {
				echo "Test"
			}
		}
		stage('integration test') {
			steps {
				echo "integration Test"
		    }
		}
     }
}
	